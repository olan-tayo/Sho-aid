{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "testEnvironment": "jsdom",
  "extensionsToTreatAsEsm": [".ts", ".tsx"],
  "moduleNameMapper": {
    "\\.css$": "identity-obj-proxy"
  }
}
